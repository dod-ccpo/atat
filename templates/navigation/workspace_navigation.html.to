      subnav=[
        {
          "label": "Add New Project",
          "href":"#",
          "active": matchesPath('workspaces/projects/new'),
          "icon": "plus"
        }
      ]
    )%}

    {% module SidenavItem(
      "Members",
      href=reverse_url('workspace_members', '123456'),
      active=matchesPath('\/workspaces\/[A-Za-z0-9]*\/members'),
